  "name": "devdocai-frontend",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build-storybook": "storybook build",
    "build:analyze": "ANALYZE=true vite build",
    "build:perf": "npm run build && npm run lighthouse",
    "coverage": "c8 npm run test:run",
    "coverage:report": "c8 report --reporter=html --reporter=text",
    "dev": "vite",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lighthouse": "lighthouse http://localhost:4173 --output-path=./lighthouse-report.html --view",
    "lint": "eslint . --ext .vue,.js,.ts",
    "lint:fix": "eslint . --ext .vue,.js,.ts --fix",
    "perf:budget": "npx bundlesize",
    "pre-commit": "npm run quality",
    "preview": "vite preview",
    "quality": "npm run lint && npm run format:check && npm run type-check && npm run test:run",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:a11y": "playwright test tests/e2e/accessibility",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:install": "playwright install",
    "test:performance": "playwright test tests/e2e/performance",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:validate": "node scripts/test-validation.js",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch"
  },
  "type": "module",
  "version": "0.0.0"
}
