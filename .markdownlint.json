{
  "default": true,
  "line-length": false,
  "no-hard-tabs": false,
  "no-trailing-spaces": {
    "br_spaces": 2,
    "list_item_empty_lines": false
  },
  "no-multiple-blanks": {
    "maximum": 2
  },
  "no-bare-urls": false,
  "blanks-around-headings": {
    "lines_above": 1,
    "lines_below": 1
  },
  "heading-increment": true,
  "list-indent": true,
  "ul-style": {
    "style": "dash"
  },
  "ol-prefix": {
    "style": "ordered"
  },
  "no-inline-html": false,
  "first-line-heading": false,
  "commands-show-output": false,
  "no-duplicate-heading": false,
  "no-emphasis-as-heading": false,
  "code-block-style": {
    "style": "fenced"
  },
  "code-fence-style": {
    "style": "backtick"
  },
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2,
    "start_indented": false
  },
  "MD013": false,
  "MD022": {
    "lines_above": 1,
    "lines_below": 1
  },
  "MD024": false,
  "MD025": {
    "level": 1
  },
  "MD026": {
    "punctuation": ".,;:"
  },
  "MD029": {
    "style": "ordered"
  },
  "MD030": {
    "ul_single": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ol_multi": 1
  },
  "MD033": false,
  "MD034": false,
  "MD036": false,
  "MD041": false,
  "MD046": {
    "style": "fenced"
  },
  "MD048": {
    "style": "backtick"
  },
  "MD049": {
    "style": "underscore"
  },
  "MD050": {
    "style": "asterisk"
  }
}