{
  "name": "devdocai",
  "version": "3.0.0",
  "description": "AI-powered documentation generation and analysis for solo developers",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "build:react": "webpack --mode production",
    "dev:react": "webpack serve --mode development",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "lint:md": "markdownlint '**/*.md' --ignore node_modules --ignore .pytest_cache",
    "lint:md:fix": "markdownlint '**/*.md' --fix --ignore node_modules --ignore .pytest_cache",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "benchmark": "ts-node scripts/benchmark-m001.ts",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "documentation",
    "ai",
    "developer-tools",
    "automation",
    "react",
    "accessibility",
    "privacy-first"
  ],
  "author": "DevDocAI Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "css-loader": "^6.8.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "jest-transform-stub": "^2.0.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@mui/system": "^5.14.0",
    "@mui/x-charts": "^6.0.0",
    "@mui/x-data-grid": "^6.10.0",
    "@types/d3": "^7.4.0",
    "d3": "^7.8.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^10.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "recharts": "^2.8.0",
    "zod": "^3.22.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
